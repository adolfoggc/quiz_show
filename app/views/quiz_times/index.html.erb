<p id="notice"><%= notice %></p>

<h1>Quiz Times</h1>

<table>
  <thead>
    <tr>
      <th>Questions</th>
      <th>Correct answers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quiz_times.each do |quiz_time| %>
      <tr>
        <td><%= quiz_time.questions %></td>
        <td><%= quiz_time.correct_answers %></td>
        <td><%= link_to 'Show', quiz_time %></td>
        <td><%= link_to 'Edit', edit_quiz_time_path(quiz_time) %></td>
        <td><%= link_to 'Destroy', quiz_time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quiz Time', new_quiz_time_path %>
